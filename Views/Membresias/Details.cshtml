@model proyectoWeb_GYM.Models.Membresia

@{
    ViewData["Title"] = "Detalle de Membresia";
}

<h1 class="text-center">Detalle de Membresia</h1>

<div style="border-radius: 15px; box-shadow:  20px 20px 60px #858080,
             -20px -20px 60px #b3adad; background: #9c9696;">
    <hr />
    <dl class="row pl-3">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nombre_membresia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nombre_membresia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.precio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.vendidas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.vendidas)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning mt-2" asp-action="Edit" asp-route-id="@Model?.id_membresia">Editar</a> |
    <a class="btn btn-primary mt-2" asp-action="Index">Regresar a la lista</a>
</div>
